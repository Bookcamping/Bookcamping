<% title "Resultados para #{resource_class.name}s" %>
<h1><%= resource_class.name %> <small>(<%= resource_class.count %>)</small></h1>

<h3>Column:<%= @search[:column] %></h3>
<h3>Value:<%= @search[:value] %></h3>


<div class="pagination">
  <%= paginate @search[:results] %>
</div>

<%= render 'index', locals: {resources: @search[:results] } %>

