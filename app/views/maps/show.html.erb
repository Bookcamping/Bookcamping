<div id="playground">
  <div class="draggable">
    <%= image_tag 'bookcamping-mapa.jpg', alt: 'mapa bookcamping',
                  width: "2200", height: "1616" %>

    <a href="/" style="left:51px;top:54px;width:538px;height:137px;">#bookcamping</a>

    <% @lists.each do |list| %>
    <%= link_to list.name, list, style: @map[list.id] %>
