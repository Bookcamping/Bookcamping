
<%= lists_div 2 do %>
<%= render_info %>
    <div class="list users">
      <%= title t('.title'), :h2 %>
      <ul>
        <% users.each do |user| %>
        <li>
          <%= link_to user.name, user %> <%= content_tag :span, 'admin', :class => 'mini' if user.admin? %>
          <%= link_to 'Editar', [:edit, user] if current_user? :super %>
        </li>
        <% end %>
      </ul>
    </div>
<% end %>