<%= title user.name %>

<% present user do |presenter| -%>
    <%= div_for user do %>
        <%= presenter.render_info %>
    <% end %>
<% end %>


<table class="shelves">
  <% user.camp_shelves.each do |shelf| %>
      <%= render_shelf shelf, shelf %>
  <% end %>
  <% user.public_shelves.each do |shelf| %>
      <%= render_shelf shelf, [user, shelf] %>
  <% end %>
</table>

