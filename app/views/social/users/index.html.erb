<% title 'Colaboradoxs #bookcamping' %>
<h1>Colaboradorxs #bookcamping <small><%= users.count %></small></h1>

<div class="users">
  <% users.each do |user| %>
      <%= div_for user do %>
          <div class="avatar">&nbsp;</div>
          <div class="name">
            <%= link_to user.name, user %>
          </div>
      <% end %>
  <% end %>
</div>