
h1.logo-bookcamping {
    width: 1025px; height: 140px;
    background: url(<%= asset_path 'logo-bookcamping.png' %>) no-repeat;
}
h1.logo-videocamping {
    width: 1025px; height: 140px;
    background: url(<%= asset_path 'logo-videocamping.png' %>) no-repeat;
}

div#go-camping {
    a.go-videocamping {width: 119px; height: 25px; background: url(<%= asset_path 'go-videocamping.png' %>) no-repeat; }
    a.go-bookcamping {width: 129px; height: 25px; background: url(<%= asset_path 'go-bookcamping.png' %>) no-repeat; }
}

a#go-map {
    display: block; margin-top: 20px;
    padding-top: 82px; overflow: visible;
    background: url(<%= asset_path 'go-map.png' %>) no-repeat;
}
